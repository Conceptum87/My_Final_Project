create_README: 
	echo "# The Final UNIX Workbench course project" > README.md
	echo " " >> README.md
	date >> track_file.txt
	cat track_file.txt >> README.md
	echo " " >> README.md
	echo "- The number of lines in guessinggame.sh is:" >> README.md
	cat guessinggame.sh | wc -l >> README.md
clean: 
	rm README.md
	rm track_file.txt	
